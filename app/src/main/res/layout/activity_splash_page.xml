<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="846dp"
android:layout_height="411dp"
tools:context=".MainActivity"
app:srcCompat="@drawable/activity_splash"
android:screenOrientation="landscape"
android:id="@+id/main_layout"
android:clickable="true"
android:onClick="onScreenClick">

<RelativeLayout
    android:id="@+id/android_main"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/activity_splash"
    android:clipToOutline="true"
    tools:ignore="MissingConstraints,RedundantNamespace,UnusedAttribute"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="0dp">
</RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
